<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>EnotesUsersServlet</servlet-name>
        <servlet-class>Enotes.controller.EnotesUsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnotesRegistrationServlet</servlet-name>
        <servlet-class>Enotes.controller.EnotesRegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddNotesServlet</servlet-name>
        <servlet-class>Enotes.controller.AddNotesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnotesAddNotesServlet</servlet-name>
        <servlet-class>Enotes.controller.EnotesAddNotesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnotesShowingAllNotesServlet</servlet-name>
        <servlet-class>Enotes.controller.EnotesShowingAllNotesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnotesUpdateDataServlet</servlet-name>
        <servlet-class>Enotes.controller.EnotesUpdateDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnotesDeleteDataServlet</servlet-name>
        <servlet-class>Enotes.controller.EnotesDeleteDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EnotesUsersServlet</servlet-name>
        <url-pattern>/EnotesUsersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnotesRegistrationServlet</servlet-name>
        <url-pattern>/EnotesRegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddNotesServlet</servlet-name>
        <url-pattern>/AddNotesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnotesAddNotesServlet</servlet-name>
        <url-pattern>/EnotesAddNotesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnotesShowingAllNotesServlet</servlet-name>
        <url-pattern>/EnotesShowingAllNotesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnotesUpdateDataServlet</servlet-name>
        <url-pattern>/EnotesUpdateDataServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnotesDeleteDataServlet</servlet-name>
        <url-pattern>/EnotesDeleteDataServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
